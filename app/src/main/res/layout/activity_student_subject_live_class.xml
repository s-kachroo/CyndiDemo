<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".subjectActivites.StudentSubjectLiveClassActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/topBar1"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:background="@color/colorStudent"
        android:elevation="5dp"
        android:padding="10dp"
        android:transitionName="TopBar"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tv_student_subject_live_class_heading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Live Class"
            android:textColor="#fff"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</LinearLayout>