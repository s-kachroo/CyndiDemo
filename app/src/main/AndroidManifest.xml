<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cyndidemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".StudentAllSubjectListActivity" />
        <activity android:name=".StudentEachSubjectActivity" />

        <activity android:name=".subjectActivites.StudentSubjectResourceActivity" />
        <activity android:name=".subjectActivites.StudentSubjectAssignmentActivity" />
        <activity android:name=".subjectActivites.StudentSubjectLiveClassActivity" />
        <activity android:name=".subjectActivites.StudentSubjectClassScheduleActivity" />
        <activity android:name=".subjectActivites.StudentSubjectAssessmentActivity" />
    </application>
</manifest>